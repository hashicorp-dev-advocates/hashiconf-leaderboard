<div>
    <Hds::AppFrame as |Frame|>
        <Frame.Header>
            <Hds::PageHeader as |PH|>
                <PH.Title ...attributes class="robot">RoboRally</PH.Title>
            </Hds::PageHeader>
        </Frame.Header>
        <Frame.Main>
            <Hds::Table ...attributes class="robot-frame" @model={{this.model}} @columns={{array (hash key=" rank"
                label="Rank" ) (hash key="name" label="Team name" ) (hash key="time" label="Time" align="right" ) }}
                @valign="middle">
                <:body as |B|>
                    <B.Tr>
                        <B.Td>{{B.data.rank}}</B.Td>
                        <B.Td>{{B.data.name}}</B.Td>
                        <B.Td @align="right">{{B.data.time}}</B.Td>
                    </B.Tr>
                </:body>
            </Hds::Table>
        </Frame.Main>
        <Frame.Footer>
            <footer class="robot-footer"></footer>
        </Frame.Footer>
    </Hds::AppFrame>
</div>